<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.jdfo.domain.SongAlbum" table="song_album" schema="yinyue">
        <id name="id">
            <column name="id" sql-type="int(11)"/>
            <generator class="identity"/>
        </id>
        <property name="albumId">
            <column name="album_id" sql-type="int(11)" not-null="false"/>
        </property>
        <property name="songId">
            <column name="song_id" sql-type="int(11)" not-null="false"/>
        </property>
    </class>
</hibernate-mapping>